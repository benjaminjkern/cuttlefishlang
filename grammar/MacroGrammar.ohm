CuttlefishMacros {
	Module = Lines+ end
    Lines = MacroLine | nonMacroLine | emptyLine
    MacroLine = "#!" Exp
    nonMacroLine= (~"\n" any)+ ("\n" | end)
    emptyLine = "\n"

    Exp = "(" func Body ")"
    Body = (Tuple | Exp | id )+
    
    Tuple = "{" ListOf<id,","> "}" 
    
    func = id

    id = ((~space  ~"]" ~"," ~")" ~"}") any)+
}
